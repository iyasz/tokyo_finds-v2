<div class="header mb-6 flex items-center gap-4">
    <h1 class="text-2xl font-medium text-slate-600">Form <%= form %></h1>
    <div class="w-[1px] h-[20px] bg-slate-400 md:block hidden"></div>
    <div class="md:flex hidden items-center gap-3">
        <p class="text-indigo-500"><%= form %></p>
        <svg x-ignore="" xmlns="http://www.w3.org/2000/svg" class="size-4" fill="none" viewBox="0 0 24 24" stroke="#64748b">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
        </svg>
        <p class="text-slate-500"><%= type %></p>
    </div>
</div>